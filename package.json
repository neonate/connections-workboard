{
  "name": "connections-workboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "dependencies": {
    "@huggingface/inference": "^4.7.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "start:prod": "node start-production.js",
    "dev": "react-scripts start",
    "build": "NPM_CONFIG_REGISTRY=https://registry.npmjs.org npm run test:ci && react-scripts build",
    "build:full": "NPM_CONFIG_REGISTRY=https://registry.npmjs.org npm run test:ci && react-scripts build && cd backend && NPM_CONFIG_REGISTRY=https://registry.npmjs.org npm install",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "test:parser": "scripts/test-parser-regression.sh",
    "test:parser:baseline": "cd tests/integration && node capture-baseline.js",
    "test:parser:compare": "cd tests/integration && node capture-baseline.js compare",
    "test:parser:update-timestamps": "cd tests/integration && node capture-baseline.js update-timestamps",
    "test:parser:help": "cd tests/integration && node capture-baseline.js help",
    "test:all": "npm run test:ci && npm run test:parser",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
